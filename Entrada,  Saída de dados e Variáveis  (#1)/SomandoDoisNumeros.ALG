Algoritmo "SomandoDoisNumeros"

Var

   N1, N2: Real
   NFinal: Real
   
Inicio

   Escreval ("SOMANDO DOIS NUMEROS")
   Escreva ("Digite um número: ")
   Leia (N1)
   Escreva ("Digite um segundo número: ")
   Leia (N2)
   NFinal <- N1 + N2
   Escreval ("A soma entre", N1 ," e", N2 ," é")
   Escreval
   Escreval
   Escreval ("          ", NFinal)
   Escreval
   Escreval
   
FimAlgoritmo