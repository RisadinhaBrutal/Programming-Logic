Algoritmo "FazendoMedia"

Var

   Valor1, Valor2: Inteiro
   MediaFinal: Real
   MediaFinal <- (valor1 + valor2) / 2
   
Inicio

   Escreval ("***Descobrindo a Média de Dois Números***")
   Escreva ("Escreva um Número: ")
   Leia (Valor1)
   Escreva ("Escreva o Segundo Número: ")
   Leia (Valor2)
   Escreva ("A Média Entre", Valor1 ,"e", Valor2 ,"é", MediaFinal)

FimAlgoritmo